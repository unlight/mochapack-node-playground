{
    "sourceMaps": false,
    "module": {
        "type": "es6"
    },
    "jsc": {
        "externalHelpers": true,
        "keepClassNames": true,
        "target": "es2020",
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        }
    }
}
